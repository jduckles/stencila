{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.ts": [
      "@parcel/transformer-typescript-tsc"
    ],
    "*.svg": [
      "@parcel/transformer-inline-string"
    ]
  },
  "compressors": {
    "*.{ts,js,css,svg}": [
      "...",
      "@parcel/compressor-gzip",
      "@parcel/compressor-brotli"
    ]
  }
}
