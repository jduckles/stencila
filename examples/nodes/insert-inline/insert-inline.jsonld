{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "Some "
        },
        {
          "@type": "stencila:InsertInline",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "inserted "
            }
          ]
        },
        {
          "@type": "Text",
          "value": "text."
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "Several inserted inlines"
        },
        {
          "@type": "stencila:InsertInline",
          "stencila:content": [
            {
              "@type": "stencila:Strong",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": "strongly emphasized"
                }
              ]
            },
            {
              "@type": "Text",
              "value": " and "
            },
            {
              "@type": "stencila:Superscript",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": "superscripted"
                }
              ]
            },
            {
              "@type": "Text",
              "value": " and "
            },
            {
              "@type": "stencila:CodeExpression",
              "stencila:code": "2 + 2"
            }
          ]
        },
        {
          "@type": "Text",
          "value": "."
        }
      ]
    }
  ]
}