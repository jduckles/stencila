{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "With a number output: "
        },
        {
          "@type": "stencila:CodeExpression",
          "stencila:code": "6 * 7",
          "programmingLanguage": "python",
          "stencila:output": 42
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "With a string output: "
        },
        {
          "@type": "stencila:CodeExpression",
          "stencila:code": "'a string'",
          "programmingLanguage": "python",
          "stencila:output": "a string"
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "With an array output: "
        },
        {
          "@type": "stencila:CodeExpression",
          "stencila:code": "[1, 2, 3]",
          "programmingLanguage": "python",
          "stencila:output": [
            1,
            2,
            3
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "With an object output: "
        },
        {
          "@type": "stencila:CodeExpression",
          "stencila:code": "dict(a=1,b=dict())",
          "programmingLanguage": "python",
          "stencila:output": {
            "a": 1,
            "b": {}
          }
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "With no language: "
        },
        {
          "@type": "stencila:CodeExpression",
          "stencila:code": "a + b"
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "With automatic execution behavior specified "
        },
        {
          "@type": "stencila:CodeExpression",
          "stencila:autoExec": "Never",
          "stencila:code": "c * d",
          "programmingLanguage": "javascript"
        }
      ]
    }
  ]
}