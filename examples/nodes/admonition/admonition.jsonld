{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Admonition",
      "stencila:admonitionType": "Note",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "A note"
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Admonition",
      "stencila:admonitionType": "Warning",
      "headline": [
        {
          "@type": "Text",
          "value": "With a title"
        }
      ],
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "A warning"
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Admonition",
      "stencila:admonitionType": "Success",
      "stencila:isFolded": false,
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "Foldable, but unfolded"
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Admonition",
      "stencila:admonitionType": "Danger",
      "headline": [
        {
          "@type": "Text",
          "value": "Folded with a title"
        }
      ],
      "stencila:isFolded": true,
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "Foldable"
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Admonition",
      "stencila:admonitionType": "Tip",
      "headline": [
        {
          "@type": "Text",
          "value": "Nested admonitions"
        }
      ],
      "stencila:content": [
        {
          "@type": "stencila:Admonition",
          "stencila:admonitionType": "Info",
          "stencila:content": [
            {
              "@type": "stencila:Admonition",
              "stencila:admonitionType": "Success",
              "stencila:content": [
                {
                  "@type": "stencila:Paragraph",
                  "stencila:content": [
                    {
                      "@type": "Text",
                      "value": "This is the first inner-inner admonition"
                    }
                  ]
                }
              ]
            },
            {
              "@type": "stencila:Admonition",
              "stencila:admonitionType": "Error",
              "stencila:content": [
                {
                  "@type": "stencila:Paragraph",
                  "stencila:content": [
                    {
                      "@type": "Text",
                      "value": "This is the second inner-inner admonition"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}