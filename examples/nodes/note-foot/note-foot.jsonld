{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "This example only has footnotes (not end or side notes) for compatibility with Markdown roundtrip conversions: "
        },
        {
          "@type": "stencila:Note",
          "stencila:noteType": "Footnote",
          "stencila:content": [
            {
              "@type": "stencila:Paragraph",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": "A paragraph"
                }
              ]
            }
          ]
        },
        {
          "@type": "Text",
          "value": ", a "
        },
        {
          "@type": "stencila:Note",
          "stencila:noteType": "Footnote",
          "stencila:content": [
            {
              "@type": "stencila:Paragraph",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": "First paragraph."
                }
              ]
            },
            {
              "@type": "stencila:Paragraph",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": "Second paragraph."
                }
              ]
            }
          ]
        },
        {
          "@type": "Text",
          "value": ", a "
        },
        {
          "@type": "stencila:Note",
          "stencila:noteType": "Footnote",
          "stencila:content": [
            {
              "@type": "stencila:Paragraph",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": "A paragraph."
                }
              ]
            },
            {
              "@type": "stencila:CodeBlock",
              "stencila:code": "A code block"
            },
            {
              "@type": "stencila:QuoteBlock",
              "stencila:content": [
                {
                  "@type": "stencila:Paragraph",
                  "stencila:content": [
                    {
                      "@type": "Text",
                      "value": "A quote block"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "@type": "Text",
          "value": "."
        }
      ]
    }
  ]
}