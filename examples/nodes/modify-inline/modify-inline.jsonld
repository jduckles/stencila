{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "This is a suggested modification to some inline code: "
        },
        {
          "@type": "stencila:ModifyInline",
          "stencila:content": [
            {
              "@type": "stencila:CodeInline",
              "stencila:code": "Some code"
            }
          ],
          "stencila:operations": [
            {
              "@type": "stencila:ModifyOperation",
              "target": "code",
              "value": {
                "@type": "stencila:StringPatch",
                "stencila:operations": [
                  {
                    "@type": "stencila:StringOperation",
                    "stencila:startPosition": 0,
                    "value": "// "
                  }
                ]
              }
            },
            {
              "@type": "stencila:ModifyOperation",
              "target": "programmingLanguage",
              "value": "python"
            }
          ]
        },
        {
          "@type": "Text",
          "value": "."
        }
      ]
    }
  ]
}