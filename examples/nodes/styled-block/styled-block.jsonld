  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "A styled block with two paragraphs:"
        }
      ]
    },
    {
      "@type": "stencila:StyledBlock",
      "stencila:code": "rounded border border-solid border-blue-200 bg-blue-100 p-2",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "Styled paragraph one."
            }
          ]
        },
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "Styled paragraph two."
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "With style language specified:"
        }
      ]
    },
    {
      "@type": "stencila:StyledBlock",
      "stencila:code": "color:red",
      "stencila:styleLanguage": "css",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "Styled paragraph."
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "Nested styled blocks:"
        }
      ]
    },
    {
      "@type": "stencila:StyledBlock",
      "stencila:code": "bg-red-100 p-2",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": "Outer paragraph."
            }
          ]
        },
        {
          "@type": "stencila:StyledBlock",
          "stencila:code": "bg-blue-100",
          "stencila:content": [
            {
              "@type": "stencila:Paragraph",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": "Inner paragraph."
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": "Containing variable interpolations:"
        }
