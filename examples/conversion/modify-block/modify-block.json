{
  "$schema": "https://stencila.dev/Article.schema.json",
  "type": "Article",
  "content": [
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "The following code chunk has a suggested modifications."
        }
      ]
    },
    {
      "type": "ModifyBlock",
      "content": [
        {
          "type": "CodeChunk",
          "programmingLanguage": "python",
          "code": "# Some code"
        }
      ],
      "operations": [
        {
          "type": "ModifyOperation",
          "target": "programmingLanguage",
          "value": "javascript"
        },
        {
          "type": "ModifyOperation",
          "target": "code",
          "value": {
            "type": "StringPatch",
            "operations": [
              {
                "type": "StringOperation",
                "startPosition": 0,
                "endPosition": 1,
                "value": "//"
              }
            ]
          }
        }
      ]
    }
  ]
}
