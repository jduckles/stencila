{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:InsertBlock",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "Proposed inserted paragraph"
              }
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:InsertBlock",
      "stencila:feedback": "Some feedback on suggestion",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "Proposed inserted paragraph with feedback"
              }
            }
          ]
        }
      ]
    }
  ]
}